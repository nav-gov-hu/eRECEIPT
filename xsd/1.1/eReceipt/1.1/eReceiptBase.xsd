<xs:schema targetNamespace="http://schemas.nav.gov.hu/ERECEIPT/1.1/base" elementFormDefault="qualified"
           attributeFormDefault="unqualified" xmlns="http://schemas.nav.gov.hu/ERECEIPT/1.1/base"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://schemas.nav.gov.hu/NTCA/1.0/common"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.nav.gov.hu/NTCA/1.0/common" schemaLocation="../1.0/common.xsd"/>
  <xs:simpleType name="APNumberType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">AP szám típus</xs:documentation>
      <xs:documentation xml:lang="en">AP number type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="9"/>
      <xs:maxLength value="9"/>
      <xs:pattern value="[A-Z][0-9]{8}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CustomerAppEventCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Vevői applikáció eseménykód típus</xs:documentation>
      <xs:documentation xml:lang="en">Customer application event code type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="5"/>
      <xs:maxLength value="5"/>
      <xs:pattern value="[A-Z][0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Digit4Type">
    <xs:annotation>
      <xs:documentation xml:lang="hu">4 jegyű szám típus</xs:documentation>
      <xs:documentation xml:lang="en">4 digit number type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="4"/>
      <xs:maxLength value="4"/>
      <xs:pattern value="[0-9]{4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="EANType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">8 vagy 13 jegyű EAN kód típus</xs:documentation>
      <xs:documentation xml:lang="en">8 or 13 digit EAN code type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="8"/>
      <xs:maxLength value="13"/>
      <xs:pattern value="[0-9]{8}|[0-9]{13}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ProcessIdentifierType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">EPD folyamat azonosító</xs:documentation>
      <xs:documentation xml:lang="en">EPD process identifier</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="10"/>
      <xs:minLength value="10"/>
      <xs:pattern value="[A-Z0-9]{10}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AtomicCsrType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Atomi CSR típus</xs:documentation>
      <xs:documentation xml:lang="en">Atomic CSR type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary">
      <xs:minLength value="1"/>
      <xs:maxLength value="8192"/>
      <xs:pattern value=".*[^\s].*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="File512kBinaryType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Bináris fájl típus, max 512kB</xs:documentation>
      <xs:documentation xml:lang="en">Binary file type, max 512kB</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary">
      <xs:maxLength value="524288"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FileExtensionType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Fájl kiterjesztés típus</xs:documentation>
      <xs:documentation xml:lang="en">File extension type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="10"/>
      <xs:pattern value="[\w,-]*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CancellationReasonType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Sztornózás ok típus</xs:documentation>
      <xs:documentation xml:lang="en">Reason of cancellation type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType15">
      <xs:enumeration value="S1">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Ügyfél elállása</xs:documentation>
          <xs:documentation xml:lang="en">Customer cancellation</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S2">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Kezelői hiba: téves bevitel</xs:documentation>
          <xs:documentation xml:lang="en">User error: incorrect input</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S3">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Kezelői hiba: téves fizetőeszköz bevitel</xs:documentation>
          <xs:documentation xml:lang="en">User error: incorrect instrument input</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S4">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Kezelői hiba: termék nincs készleten</xs:documentation>
          <xs:documentation xml:lang="en">User error: product out of stock</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S5">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Technikai: téves bizonylattípus kibocsátása</xs:documentation>
          <xs:documentation xml:lang="en">Technical: incorrect document category</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S6">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Technikai: sikertelen fizetőeszköz használat</xs:documentation>
          <xs:documentation xml:lang="en">Technical: unsuccesfull instrument input</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S7">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Technikai: téves ügyfél adat/hibás bevitel</xs:documentation>
          <xs:documentation xml:lang="en">Technical: incorrect customer data input</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S8">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Technikai: próbavásárlás</xs:documentation>
          <xs:documentation xml:lang="en">Technical: test purchase</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="S0">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb</xs:documentation>
          <xs:documentation xml:lang="en">Other</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CashPaymentTitleType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Pénztári befizetés-kifizetés vagy fizetőeszköz csere jogcímei típus</xs:documentation>
      <xs:documentation xml:lang="en">Cash payment title type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType15">
      <xs:enumeration value="01">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Váltópénz bevitel</xs:documentation>
          <xs:documentation xml:lang="en">Cash coin input</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="02">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Pénztáros pénzfelvétel</xs:documentation>
          <xs:documentation xml:lang="en">Withdraw by cashier (in)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="03">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Díjbeszedés</xs:documentation>
          <xs:documentation xml:lang="en">Collection</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="04">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Sorsjegy eladás</xs:documentation>
          <xs:documentation xml:lang="en">Sale of lottery ticket</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="05">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Előleg</xs:documentation>
          <xs:documentation xml:lang="en">Advance payment</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="06">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Pénztár hiány</xs:documentation>
          <xs:documentation xml:lang="en">Cash deficit</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="07">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Borravaló</xs:documentation>
          <xs:documentation xml:lang="en">Tip</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="08">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb befizetés</xs:documentation>
          <xs:documentation xml:lang="en">Other payment (in)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="31">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Fölözés</xs:documentation>
          <xs:documentation xml:lang="en">Skimming</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="32">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Pénztáros levétel</xs:documentation>
          <xs:documentation xml:lang="en">Withdraw by cashier (out)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="33">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Utalvány kivét</xs:documentation>
          <xs:documentation xml:lang="en">Voucher take out</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="34">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Ajándékkártya kivét</xs:documentation>
          <xs:documentation xml:lang="en">Gift card take out</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="35">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Bérkifizetés</xs:documentation>
          <xs:documentation xml:lang="en">Salary payment</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="36">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Munkabér előleg</xs:documentation>
          <xs:documentation xml:lang="en">Pre-payment of salary</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="37">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Postaköltség</xs:documentation>
          <xs:documentation xml:lang="en">Postal cost</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="38">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb rezsi</xs:documentation>
          <xs:documentation xml:lang="en">Other costs</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="39">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Áruvásárlás</xs:documentation>
          <xs:documentation xml:lang="en">Purchasing of goods </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="40">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Záróösszeg levétel</xs:documentation>
          <xs:documentation xml:lang="en">Withdraw of closing balance</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="41">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb kifizetés</xs:documentation>
          <xs:documentation xml:lang="en">Other payments (out)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="42">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Készpénzfelvétel</xs:documentation>
          <xs:documentation xml:lang="en">Cashback</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="60">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Fizetőeszköz csere</xs:documentation>
          <xs:documentation xml:lang="en">Change of payment instrument</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CollectorCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Forgalmi gyüjtő típus</xs:documentation>
      <xs:documentation xml:lang="en">Collector code type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="3"/>
      <xs:pattern value="[A-E]|N|TAM|AAM|EAM|ATK|TRA|SEC|ART|ANT|EUE|HO"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CustomerVatStatusType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Vevő ÁFA szerinti státusz típusa</xs:documentation>
      <xs:documentation xml:lang="en">Customers status type by VAT</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType15">
      <xs:enumeration value="DOMESTIC">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Belföldi ÁFA alany</xs:documentation>
          <xs:documentation xml:lang="en">Domestic VAT subject</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OTHER">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb (belföldi nem ÁFA alany, nem természetes személy, külföldi ÁFA alany és külföldi nem ÁFA alany, nem természetes személy)</xs:documentation>
          <xs:documentation xml:lang="en">Other (domestic non-VAT subject, non-natural person, foreign VAT subject and foreign non-VAT subject, non-natural person)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PRIVATE_PERSON">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Nem ÁFA alany (belföldi vagy külföldi) természetes személy</xs:documentation>
          <xs:documentation xml:lang="en">Non-VAT subject (domestic or foreign) natural person</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DataNameType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az adatmező egyedi azonosító típusa</xs:documentation>
      <xs:documentation xml:lang="en">Unique identification type of the data field</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType255">
      <xs:minLength value="1"/>
      <xs:maxLength value="255"/>
      <xs:pattern value="[A-Z][0-9]{5}[_][_A-Z0-9]{1,249}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DocumentCategoryType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A bizonylat típusa</xs:documentation>
      <xs:documentation xml:lang="en">Type of document</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="50"/>
      <xs:enumeration value="RECEIPT">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Nyugta</xs:documentation>
          <xs:documentation xml:lang="en">Receipt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HEALTH">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egészségkártya</xs:documentation>
          <xs:documentation xml:lang="en">Health</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FUEL">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Üzemanyagkártya</xs:documentation>
          <xs:documentation xml:lang="en">Fuel</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CONSUME">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Fogyasztási összesítő</xs:documentation>
          <xs:documentation xml:lang="en">Consumption summary</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OTHER">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Other</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DEFERRED">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Szállodai átterhelés bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Hotel deferred document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DocumentClassType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Bizonylat típus, milyen bizonylatot küld be az e-pénztárgép</xs:documentation>
      <xs:documentation xml:lang="en">Document class type, what document is sent by the e-cash register</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="50"/>
      <xs:enumeration value="RECEIPT">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Nyugta bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Receipt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FUEL">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Üzemanyagkártyás értékesítési bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Fuel card sales receipt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HEALTH">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egészségkártyás értékesítési bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Health Fund sales receipt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DEFERRED">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Szállodai átterhelési bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Hotel deferred document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CONSUME">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Fogyasztási összesítő bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Consumption summary document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SIMPLIFIEDINVOICE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyszerűsített számla</xs:documentation>
          <xs:documentation xml:lang="en">Simplified invoice</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NORMALINVOICE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Normál számla</xs:documentation>
          <xs:documentation xml:lang="en">Normal invoice</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OTHER">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéb bizonylat</xs:documentation>
          <xs:documentation xml:lang="en">Other document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DocumentDateType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Dátum típus</xs:documentation>
      <xs:documentation xml:lang="en">Date type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date">
      <xs:minInclusive value="2010-01-01"/>
      <xs:pattern value="\d{4}-\d{2}-\d{2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DocumentNumberType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Dokumentum vagy módosító okirat sorszám típus</xs:documentation>
      <xs:documentation xml:lang="en">Sequential number type of the original document or modification document</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="100"/>
      <xs:pattern value="((UK|EK|SZ|NY|PM|PJ|SA|FO|OT)-[A-Z][0-9]{8}/[0-9]{8}/[0-9]{4}/[0-9]{5})|((NN|NZ)-[A-Z][0-9]{8}/[0-9]{8}/[0-9]{4})">
        <xs:annotation>
          <xs:documentation xml:lang="hu">
        NN - Napnyitás bizonylat
        UK - Üzemanyagkártyás értékesítés
        EK - Egészségkártyás értékesítés
        SZ - Számla vagy Egyszerűsített számla
        NY - Nyugta
        PM - Pénzmozgás bizonylat
        PJ - Pénztár jelentés
        NZ - Napi forgalmi jelentés
        SA - Szállodai átterhelés bizonylat
        FO - Fogyasztási összesítő bizonylat
        OT - Egyéb bizonylat
        </xs:documentation>
          <xs:documentation xml:lang="en">
        NN - Day open report
        UK - Fuel card sale
        EK - Health Fund sale
        SZ - Invoice or Simplified invoice
        NY - Receipt
        PM - Cashflow document
        PJ - Cashregister report
        NZ - Daily cashflow report
        SA - Hotel deferred document
        FO - Consumption summary
        OT - Other document
        </xs:documentation>
        </xs:annotation>
      </xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DocumentOperationType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Dokumentum művelet típus</xs:documentation>
      <xs:documentation xml:lang="en">Document operation type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType8">
      <xs:enumeration value="CREATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Eredeti documentum</xs:documentation>
          <xs:documentation xml:lang="en">Original document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MODIFY">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eredeti dokumentumot módosító okirat</xs:documentation>
          <xs:documentation xml:lang="en">Modification of original document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STORNO">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eredeti documentum érvénytelenítése</xs:documentation>
          <xs:documentation xml:lang="en">Cancellation of original document</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DocumentUnboundedIndexType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Sorszám típus</xs:documentation>
      <xs:documentation xml:lang="en">Index type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="EncryptedSymmetricInitialVectorType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A nyugta kiegészítő részének titkosításához használt szimmetrikus kulcs típus</xs:documentation>
      <xs:documentation xml:lang="en">Symmetric key used to encrypt the additional part of the receipt type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary"/>
  </xs:simpleType>
  <xs:simpleType name="EncryptedSymmetricKeyType">
    <xs:annotation>
