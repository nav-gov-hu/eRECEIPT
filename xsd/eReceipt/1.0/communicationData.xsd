<!--
# Project: NAV e-Pénztárgép rendszer communicationData XML séma
# Author: NAV Informatikai Intézet

# Version: v1.0 2023/08/23
-->
<xs:schema targetNamespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/communicationData" elementFormDefault="qualified"
           attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://schemas.nav.gov.hu/ERECEIPT/1.0/communicationData"
           xmlns:common="http://schemas.nav.gov.hu/NTCA/1.0/common"
           xmlns:base="http://schemas.nav.gov.hu/ERECEIPT/1.0/base">
  <xs:import namespace="http://schemas.nav.gov.hu/NTCA/1.0/common" schemaLocation="common.xsd"/>
  <xs:import namespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/base" schemaLocation="eReceiptBase.xsd"/>
  <xs:annotation>
    <xs:documentation xml:lang="hu">e-Pénztárgépek kommunikációs eseteiben használt adatok leírását tartalmazó XSD</xs:documentation>
    <xs:documentation xml:lang="en">XSD containing a description of the data used in e-cash register communication cases</xs:documentation>
  </xs:annotation>
  <xs:simpleType name="BlockUnblockType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Blokkolás típus</xs:documentation>
      <xs:documentation xml:lang="en">Block type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="BLOCK">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Blokkolás</xs:documentation>
          <xs:documentation xml:lang="en">Block</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UNBLOCK">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Blokkolás feloldás</xs:documentation>
          <xs:documentation xml:lang="en">Unblock</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CashRegisterWorkStateType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">e-Pénztárgép állapot típus</xs:documentation>
      <xs:documentation xml:lang="en">e-Cash register work state type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="OK">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az e-pénztárgép működik és nincs blokkolva</xs:documentation>
          <xs:documentation xml:lang="en">The e-cash register is working and not blocked</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BLOCK">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az e-pénztárgép blokkolt állapotban van</xs:documentation>
          <xs:documentation xml:lang="en">The e-cash register is blocked</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ERROR">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az e-pénztárgép hibás</xs:documentation>
          <xs:documentation xml:lang="en">Error on e-cash register</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="GpsType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">WGS84 szerinti GPS koordináták Fok, perc, másodperc formátumban</xs:documentation>
      <xs:documentation xml:lang="en">WGS84 GPS coordinates in degrees, minutes, seconds</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{0,2},[0-9]{4,16}|([0-8][0-9]),([0-5][0-9]),([0-5][0-9])\.([0-9][0-9])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="HttpMethodType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Http metódusok listája.</xs:documentation>
      <xs:documentation xml:lang="en">List of http methods.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="GET"/>
      <xs:enumeration value="POST"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="IncorporationType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Gazdasági típus</xs:documentation>
      <xs:documentation xml:lang="en">Incorporation type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType15">
      <xs:enumeration value="ORGANIZATION">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Gazdasági társaság</xs:documentation>
          <xs:documentation xml:lang="en">Economical company</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SELF_EMPLOYED">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Egyéni vállalkozó</xs:documentation>
          <xs:documentation xml:lang="en">Self employed private entrepreneur</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="TAXABLE_PERSON">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Adószámos magánszemély</xs:documentation>
          <xs:documentation xml:lang="en">Private person with tax number</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MobileConnectionType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Átviteli technológia típus</xs:documentation>
      <xs:documentation xml:lang="en">Mobile connection type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="GSM">
        <xs:annotation>
          <xs:documentation xml:lang="hu">GSM</xs:documentation>
          <xs:documentation xml:lang="en">GSM</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2G">
        <xs:annotation>
          <xs:documentation xml:lang="hu">2G technológia</xs:documentation>
          <xs:documentation xml:lang="en">2G technology</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2,5G">
        <xs:annotation>
          <xs:documentation xml:lang="hu">2,5G technológia</xs:documentation>
          <xs:documentation xml:lang="en">2,5G technology</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3G">
        <xs:annotation>
          <xs:documentation xml:lang="hu">3G technológia</xs:documentation>
          <xs:documentation xml:lang="en">3G technology</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4G">
        <xs:annotation>
          <xs:documentation xml:lang="hu">4G technológia</xs:documentation>
          <xs:documentation xml:lang="en">4G technology</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="5G">
        <xs:annotation>
          <xs:documentation xml:lang="hu">5G technológia</xs:documentation>
          <xs:documentation xml:lang="en">5G technology</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="PercentageType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Százalékos érték a százalékjel nélkül, szükség esetén tizedesponttal, max 2 tizedesjegyre kerekítve</xs:documentation>
      <xs:documentation xml:lang="en">Percentage without the percentage sign, with decimal point if necessary, rounded to max 2 decimal places</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="5"/>
      <xs:fractionDigits value="2"/>
      <xs:maxInclusive value="100"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="PrintMessageType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Technikai tájékoztatás interfészen átadott üzenet, amelyet a pénztárzárást követően ki kell nyomtatni a rendszer nyomtatón (PRINT) </xs:documentation>
      <xs:documentation xml:lang="en">Technical information by interface message, to be printed on the system printer (PRINT) after closing the e-cash register</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="2048"/>
      <xs:pattern value="[a-zA-Z0-9aáeéiíoóöőuúüűAÁEÉIÍOÓÖŐUÚÜŰ_*%=+§/\\@]{2048}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="PrivateKeyType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Privát kulcs típus. A kulcsot PEM formátumban tartalmazza.</xs:documentation>
      <xs:documentation xml:lang="en">Private key type. Contains the key in PEM format</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary"/>
  </xs:simpleType>
  <xs:simpleType name="PublicKeyType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Publikus kulcs típus. A kulcsot PEM formátumban tartalmazza.</xs:documentation>
      <xs:documentation xml:lang="en">Public key type.Contains the key in PEM format </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:base64Binary"/>
  </xs:simpleType>
  <xs:simpleType name="RegistrationNumberType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Üzembehelyezési kód típus</xs:documentation>
      <xs:documentation xml:lang="en">Registration number type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{16}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ServiceType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Szolgáltatás lista típus</xs:documentation>
      <xs:documentation xml:lang="en">Service list type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="BLOCK_UNBLOCK"/>
      <xs:enumeration value="DOWNLOAD_PRODUCT_LIST">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Terméktörzs letöltés</xs:documentation>
          <xs:documentation xml:lang="en">Product list download</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DOWNLOAD_SOFTWARE_UPDATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Szoftver frissítés letöltés</xs:documentation>
          <xs:documentation xml:lang="en">Software update download</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="END_OF_OPERATION">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Üzemeltetés befejezése</xs:documentation>
          <xs:documentation xml:lang="en">End of operation</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="HELLO">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Hello szolgáltatás</xs:documentation>
          <xs:documentation xml:lang="en">Hello service</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OPERATOR_SITE_UPDATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Adózói adatlekérdezés</xs:documentation>
          <xs:documentation xml:lang="en">Operator site update</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OWNER_CHANGE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Átszemélyesítés</xs:documentation>
          <xs:documentation xml:lang="en">Owner change</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PRINT_TECHNICAL_INFO">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Technikai tájékoztatás küldés</xs:documentation>
          <xs:documentation xml:lang="en">Print technical info</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="QUERY_TAXPAYER">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Belföldi adószám ellenőrzés</xs:documentation>
          <xs:documentation xml:lang="en">Query taxpayer</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="REGISTRATION">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Eszköz regisztráció</xs:documentation>
          <xs:documentation xml:lang="en">Registration</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RENEW_CERTIFICATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Authentikációs tanúsítvány megújítása</xs:documentation>
          <xs:documentation xml:lang="en">Renew certificate</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="RENEW_EXPIRED_CERTIFICATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Lejárt authentikációs tanúsítvány megújítása</xs:documentation>
          <xs:documentation xml:lang="en">Renew expired certificate</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SOFTWARE_UPDATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Szoftver frissítés</xs:documentation>
          <xs:documentation xml:lang="en">Software update</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="VAT_UPDATE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Áfa törzs lekérdezés</xs:documentation>
          <xs:documentation xml:lang="en">VAT update</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SOUND_DOWNLOAD">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Hangfálj letöltés</xs:documentation>
          <xs:documentation xml:lang="en">Wav file download</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LOGO_DOWNLOAD">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Logó letöltés</xs:documentation>
          <xs:documentation xml:lang="en">Logo download</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SoftwareIdType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A program azonosítója típus</xs:documentation>
      <xs:documentation xml:lang="en">Software ID type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="18"/>
      <xs:pattern value="[0-9A-Z\-]{18}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SoftwareOperationType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az ePénztárgép működési típusa (AE alapú (hardveres) vagy online szolgáltatás)</xs:documentation>
      <xs:documentation xml:lang="en">Software operation type (AE-based (hardware) or online)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="AE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">AE alapú (hardveres) e-pénztárgép</xs:documentation>
          <xs:documentation xml:lang="en">AE-based (hardware) e-cash register</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ONLINE_SERVICE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Online szolgáltatás</xs:documentation>
          <xs:documentation xml:lang="en">Online service</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TaxpayerAddressClassType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Adózói cím típus</xs:documentation>
      <xs:documentation xml:lang="en">Taxpayer address type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="common:AtomicStringType8">
      <xs:enumeration value="HQ">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Székhely</xs:documentation>
          <xs:documentation xml:lang="en">Headquarter</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SITE">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Telephely</xs:documentation>
          <xs:documentation xml:lang="en">Site office</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BRANCH">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Fióktelep</xs:documentation>
          <xs:documentation xml:lang="en">Branch office</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TeaorCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">TEÁOR kód típus</xs:documentation>
      <xs:documentation xml:lang="en">TEÁOR code type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{2}|[0-9]{2}.[0-9]{1}|[0-9]{2}.[0-9]{2}"/>
      <xs:minLength value="2"/>
      <xs:maxLength value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UrlType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatás szabványos URL-je (https://host:port/resource URI formátumban)</xs:documentation>
      <xs:documentation xml:lang="en">Standard URL of the service to be reqested by the e-cash register (https://host:port/resource in URI format)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="200"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AeInternalMetric">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az AE egység belső mérőszámai típus</xs:documentation>
      <xs:documentation xml:lang="en">AE internal technical indicators type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="aeInternalMetricId">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>Új mező</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">A belső mérőszám azonosítója</xs:documentation>
          <xs:documentation xml:lang="en">The AE internal metric ID</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="aeInternalMetricValue">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>OCC</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">A belső mérőszám értéke</xs:documentation>
          <xs:documentation xml:lang="en">The AE internal metric value</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AeInternalMetricListType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az AE egység belső mérőszámai lista típus</xs:documentation>
      <xs:documentation xml:lang="en">AE internal technical indicators list type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="aeInternalMetric" type="AeInternalMetric">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az AE egység belső mérőszámai</xs:documentation>
          <xs:documentation xml:lang="en">AE internal technical indicators</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AeOperationType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az AE műszaki mérőszámai típus</xs:documentation>
      <xs:documentation xml:lang="en">AE technical indicators type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="mobileConnection" type="MobileConnectionType" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>NET</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">A legutóbbi bejelentkezés során használt átviteli
                             technológia</xs:documentation>
          <xs:documentation xml:lang="en">The used transmission technology during the last login</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="provider" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>PRV</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">A legutóbbi bejelentkezés során használt
                            infrastruktúrát biztosító fizikai
                            mobilszolgáltató:
                            0 – Magyar Telekom
                            1 – Telenor
                            2 – Vodafone
                            3 – Egyéb
                            </xs:documentation>
          <xs:documentation xml:lang="en">The mobil service provider of physical infrastructure 
                                      during the last login:
                                      0 – Magyar Telekom
                                      1 – Telenor
                                      2 – Vodafone
                                      3 – Other
                            </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="0|1|2|3"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="aeInternalMetric" minOccurs="0" type="AeInternalMetricListType">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>Új mező</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">AE egység belső mérőszámai</xs:documentation>
          <xs:documentation xml:lang="en">AE internal technical indicators</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AeStateType">
    <xs:annotation>
      <xs:appinfo>
        <legacyMapping/>
      </xs:appinfo>
      <xs:documentation xml:lang="hu">AE állapot típus</xs:documentation>
      <xs:documentation xml:lang="en">AE state type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="freeStorage" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>SFL</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">Az AE-ben működő adattárolón lévő szabad tároló kapacitás (Kbyte)</xs:documentation>
          <xs:documentation xml:lang="en">The free storage capacity (Kbyte) on the AE storage device</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:nonNegativeInteger">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="signalStrength" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>SSG</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">Az AE-ben működő modem által mért hálózati térerőt
                             (-1)dBm - (-199)dBm-ig terjedő skálán negatív egész
                             számmal ábrázolva</xs:documentation>
          <xs:documentation xml:lang="en">The network signal strength measured by the modem operating
          in AE is expressed as a negative integer on a scale from (-1)dBm to (-199)dBm</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:negativeInteger">
            <xs:minInclusive value="-199"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="batteryLevel" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <legacyMapping>SAC</legacyMapping>
          </xs:appinfo>
          <xs:documentation xml:lang="hu">Akkumulátor töltöttség egész számmal, vezető nullák
                             nélkül (%)</xs:documentation>
          <xs:documentation xml:lang="en">Battery charge with integer, without leading zeros (%</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
