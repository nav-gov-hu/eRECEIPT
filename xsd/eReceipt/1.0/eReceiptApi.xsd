<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/api" elementFormDefault="qualified"
           attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://schemas.nav.gov.hu/ERECEIPT/1.0/api" xmlns:common="http://schemas.nav.gov.hu/NTCA/1.0/common"
           xmlns:communication="http://schemas.nav.gov.hu/ERECEIPT/1.0/communicationData"
           xmlns:reportMessage="http://schemas.nav.gov.hu/ERECEIPT/1.0/reportMessage"
           xmlns:base="http://schemas.nav.gov.hu/ERECEIPT/1.0/base" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.nav.gov.hu/NTCA/1.0/common" schemaLocation="common.xsd"/>
  <xs:import namespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/base" schemaLocation="eReceiptBase.xsd"/>
  <xs:import namespace="http://schemas.nav.gov.hu/ERECEIPT/1.0/communicationData"
             schemaLocation="communicationData.xsd"/>
  <xs:annotation>
    <xs:documentation xml:lang="hu">Az e-pénztárgépek által hívható szolgáltatások kérés-válasz objektumait leíró XSD.</xs:documentation>
    <xs:documentation xml:lang="en">The request and response objects of the services called be e-cash registers.</xs:documentation>
  </xs:annotation>  
  <xs:complexType name="BlockUnblockRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép blokkolását vagy blokkolásának feloldását megvalósító szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The e-cash register blocking and unblocking service request type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BlockUnblockResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép blokkolását vagy blokkolásának feloldását megvalósító szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The e-cash register blocking and unblocking service response type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
          <xs:element name="blockUnblock" type="communication:BlockUnblockType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Utasítás az e-pénztárgép blokkolására vagy blokkolásának feloldására. BLOCK esetén az e-pénztárgépet blokkolt állapotba kell állítani, UNBLOCK esetén az e-pénztárgép blokkolását fel kell oldani.</xs:documentation>
              <xs:documentation xml:lang="en">Command to block to unblock the e-cash register. In case of BLOCK the e-cash register has to be blocked, in case of UNBLOCK the e-cash register has to be unblocked.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="blockUnblockProcessId" type="base:ProcessIdentifierType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Végrehajtandó utasítás EPD folyamat azonosítója. Az állapot visszajelentésekben ezt az azonosítót kell visszaadni, az adott állapotba vezető folyamat azonosítójaként.</xs:documentation>
              <xs:documentation xml:lang="en">EPD process identifier of the instruction to be executed. This identifier must be returned in the status reports, as the identifier of the process leading to the given status.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CashRegisterInfoRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép állapot beküldésének típusa.</xs:documentation>
      <xs:documentation xml:lang="en"></xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="cashRegisterInfo" type="communication:CashRegisterInfoType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">e-Pénztárgép info típus.</xs:documentation>
              <xs:documentation xml:lang="en"></xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CashRegisterInfoResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép állapot beküldés válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en"></xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CommMgrRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatások listáját visszaadó szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the service which lists the services that the e-cash register has to call.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CommMgrResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatások listáját visszaadó szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the service which lists the services that the e-cash register has to call.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
          <xs:element name="endpoints" type="communication:EndpointsListType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép által meghívandó szolgáltatások listája.</xs:documentation>
              <xs:documentation xml:lang="en">The list of the services that the e-cash register has to call.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DocumentRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A nyugta, számla, egyszerűsített számla, és egyes speciális értékesítések bizonylatainak fogadását megvalósító szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the service which receives receipts, invoices, simplified invoices and special sale documents.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="taxNumber" type="common:TaxpayerIdType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A bizonylatot kiállító adózó adószámának első 8 karaktere, vagy ÁFA-csoport tagság esetén a bizonylatot kiállító adózó tagi adószámának első 8 karaktere.</xs:documentation>
              <xs:documentation xml:lang="en">The first 8 characters of the tax number of the taxpayer who creates the document, or (if part of a VAT group) the first 8 characters of the member tax number of the taxpayer who creates the document.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="groupIdentificationNumber" type="common:TaxpayerIdType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="hu">ÁFA-csoport tagság esetén a bizonylatot kiállító adózó csoportazonosító számának első 8 karaktere.</xs:documentation>
              <xs:documentation xml:lang="en">The first 8 characters of the group identification number of the taxpayer who creates the document (if part of a VAT group).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="documentClass" type="base:DocumentClassType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A beküldött bizonylat típusa (nyugta, számla, egyszerűsített számla, stb.)</xs:documentation>
              <xs:documentation xml:lang="en">The type of the document (recipt, invoice, simplified invoice, etc.)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="documentEnvelope" type="base:SignedDocumentEnvelopeType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A bizonylat NAV-nak és vevőnek átadandó adatai a Fejlesztői dokumentációban meghatározott formátumban.</xs:documentation>
              <xs:documentation xml:lang="en">The document data for the customer and the NAV in the format described in Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="decryptKey" type="xs:base64Binary">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Független szimetrikus kulcs a bizonylat adatok kititkosításához.</xs:documentation>
              <xs:documentation xml:lang="en">Independent symmetric key for encryption of receipt data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="searchKeyTimestamp" type="common:GenericTimestampType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A nyugtatárból ezzel az időbélyeggel kérdezhető le a bizonylat. A bizonylat kiállításának időpontját tartalmazza.</xs:documentation>
              <xs:documentation xml:lang="en">The document can be queried by this data from the Document Store. It contains the document creation timestamp.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="searchKey" type="xs:base64Binary">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A nyugtatárból ezzel a kulccsal kérdezhető le a bizonylat.</xs:documentation>
              <xs:documentation xml:lang="en">The document can be queried by this data from the Document Store.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="qRCodeExpired" type="xs:boolean">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A QR generálása a fejlesztői dokumentációban meghatározott időintervallumnál régebben készült .</xs:documentation>
              <xs:documentation xml:lang="en">QR code generate timestamp expired.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="offlineCreated" type="xs:boolean">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A hírközlő hálózat elérhetetlenségének megszűnése esetén az érintett bizonylatokat és adatszolgáltatásokat haladéktalanul el kell küldeni, a késleltetett küldés tényét ennek a mezőnek a kitöltésével kell jelezni.</xs:documentation>
              <xs:documentation xml:lang="en">Whether the document was created offline. When the communication network is online again, the document has to be sent immidiately, with this flag.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cashRegisterSignCertificate" type="communication:CertificateType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgép aláíró tanúsítványa DER formátumban. A tanúsítvány privát kulcsával kerülnek aláírásra az e-pénztárgép által küldött üzenetek. Az aláíró kulccsal kapcsolatos követelmények a Fejlesztői dokumentáció „Aláírás képzése” fejezetében találhatók.</xs:documentation>
              <xs:documentation xml:lang="en">The sign certificate of the e-cash register in DER format. The private key of this certificate has to be used to sign the messages of the e-cash register. The requirements of the sign key can be found in the "Signing" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="recordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az aktuálisan beküldött bizonylat recordCounter értéke. Leírását lásd a Fejlesztői dokumentáció „NAV ellenőrző kód képzése” fejezetében.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the actual document. See further information in the "Generate NAV verification code" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="lastRecordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgépen utoljára kiállított bizonylat recordCounter értéke. Leírását lásd a Fejlesztői dokumentáció „NAV ellenőrző kód képzése” fejezetében.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the last document in the e-cash register. See further information in the "Generate NAV verification code" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ntcaVerificationCode" type="common:SHA256Type">
            <xs:annotation>
              <xs:documentation xml:lang="hu">NAV ellenőrző kód. Leírását lásd a Fejlesztői dokumentáció „NAV ellenőrző kód képzése” fejezetében.</xs:documentation>
              <xs:documentation xml:lang="en">Ntca verification code. See further information in the "Generate NAV verification code" chapter of the Development Documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="documentEnvelopVersion" type="common:AtomicStringType15" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Amennyiben a beküldött boríték tartalmát egy régebbi interfész verzió szerint állította elő a pénztárgép (újraküldés esetén), mint a jelenlegi interfész verzió (requestVersion), akkor annak verziószámát itt meg kell megadni.</xs:documentation>
              <xs:documentation xml:lang="en">If the contents of the submitted envelope were produced by the cash register according to an older interface version (in case of resending) than the current interface version (requestVersion), then its version number must be entered here.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="sendMissingDocumentProcessId" type="base:ProcessIdentifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="hu">Amennyiben a bizonylat beküldése a SEND_MISSING_DOCUMENT felszólítás végrehajtásaként történik, akkor itt kell megadni az abban kapott folyamat azonosítót. Normál beküldés esetén nem kell megadni.</xs:documentation>
                    <xs:documentation xml:lang="en">If the document is sent due to the execution of the SEND_MISSING_DOCUMENT command, the process ID received in it must be entered here. It is not required for normal submission.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DocumentResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A nyugta, számla, egyszerűsített számla, és egyes speciális értékesítések bizonylatainak fogadását megvalósító szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the service which receives receipts, invoices, simplified invoices and special sale documents.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="taxNumber" type="common:TaxpayerIdType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A kérésben szereplő adószám.</xs:documentation>
              <xs:documentation xml:lang="en">The tax number in the request.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="QueryCertificateResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">A kiállított tanúsítvány lekérdezés válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the issued certificate query request.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="queryCertificateResult" type="communication:QueryCertificateResultType">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A tanúsítvány lekérés eredménye.</xs:documentation>
              <xs:documentation xml:lang="en">Result of the certificate query.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="certificateType" type="communication:CertificateTypeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A tanúsítvány típusa.</xs:documentation>
              <xs:documentation xml:lang="en">The type of the certificate</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="certificate" type="communication:CertificateType" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="hu">A tanúsítvány.</xs:documentation>
              <xs:documentation xml:lang="en">The certificate.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EndOfOperationRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetés befejezés szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the e-cash register end of operation service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="lastRecordCounter" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Az e-pénztárgépen az utoljára kiállított bizonylat vagy riport recordCounter értéke.</xs:documentation>
              <xs:documentation xml:lang="en">The recordCounter value of the last receipt or report issued at the e-cash register.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="coTaxpayerLastRecordCounter" minOccurs="0" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation xml:lang="hu">Kétadózós e-pénztárgép esetén a társadózó által az e-pénztárgépen utoljára kiállított bizonylat vagy riport recordCounter értéke.</xs:documentation>
              <xs:documentation xml:lang="en">In case of two taxpayers e-cash register, the recordCounter value of the last receipt or report issued by the co-taxpayer.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cashRegisterWorkState" type="communication:CashRegisterWorkStateType">
            <xs:annotation>
              <xs:appinfo>
                <legacyMapping/>
              </xs:appinfo>
              <xs:documentation xml:lang="hu">Az e-pénztárgép állapota</xs:documentation>
              <xs:documentation xml:lang="en">The state of the e-cash register</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EndOfOperationResponseType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép üzemeltetés befejezés szolgáltatás válasz típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The response type of the e-cash register end of operation service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptResponseType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános válasz típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general response type of the eReceipt system.</xs:documentation>
        </xs:annotation>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HelloRequestType">
    <xs:annotation>
      <xs:documentation xml:lang="hu">Az e-pénztárgép regisztrálását vagy átszemélyesítését követően meghívandó szolgáltatás kérés típusa.</xs:documentation>
      <xs:documentation xml:lang="en">The request type of the service which has to be called by the e-cash register after the device registration or the owner change.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="base:BasicEReceiptRequestType">
        <xs:annotation>
          <xs:documentation xml:lang="hu">Az eReceipt rendszerben alkalmazott általános kérés típus adatai.</xs:documentation>
          <xs:documentation xml:lang="en">The general request type of the eReceipt system.</xs:documentation>
        </xs:annotation>
          <xsd:sequence>
              <xsd:element name="currentOperatorSiteProcessId" type="base:ProcessIdentifierType">
                  <xsd:annotation>
                      <xsd:documentation xml:lang="hu">A Hello hívást megelőző eszközregisztráció vagy átszemélyesítés során az üzemeltetői és üzemeltetési hely adatokhoz kapott EPD folyamat azonosító (operatorSiteProcessIdentifier).</xsd:documentation>
                      <xsd:documentation xml:lang="en">The EPD process identifier received for the operator and location data in the response of the last device registration or owner change (operatorSiteProcessIdentifier).</xsd:documentation>
                  </xsd:annotation>
              </xsd:element>
              <xsd:choice>
                  <xsd:element name="currentVatProcessId" type="base:ProcessIdentifierType">
                      <xsd:annotation>
                          <xsd:documentation xml:lang="hu">A Hello hívást megelőző eszközregisztráció során az ÁFA táblázathoz kapott EPD folyamat azonosító (vatProcessIdentifier).</xsd:documentation>
                          <xsd:documentation xml:lang="en">The EPD process identifier received for the VAT table in the response of the last device registration (vatProcessIdentifier).</xsd:documentation>
                      </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="currentAeBlockUnblockStateProcessId" type="base:ProcessIdentifierType">
